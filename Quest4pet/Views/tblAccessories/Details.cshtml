@model Quest4pet.Models.tblAccessory

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>tblAccessory</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Accessory Name
        </dt>

        <dd>
            @Model.Accessories_Name
        </dd>

        <dt>
            Accessory Image
        </dt>

        <dd>
            <img src="@Url.Content(Model.Accessories_Image)" width="100" height="100" />
        </dd>

        <dt>
            Accessory Description
        </dt>

        <dd>
            @Model.Accessories_Description
        </dd>

        <dt>
            Accessory Price
        </dt>

        <dd>
            @Model.Accessories_Price
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Accessories_ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
