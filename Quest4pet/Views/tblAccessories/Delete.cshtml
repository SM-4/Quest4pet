@model Quest4pet.Models.tblAccessory

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>tblAccessory</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Accessory Name
        </dt>

        <dd>
            @Model.Accessories_Name
        </dd>

        <dt>
            Accessory Image
        </dt>

        <dd>
            @*@Model.Accessories_Image*@
            <img src="@Url.Content(Model.Accessories_Image)" width="100" height="100" />
        </dd>

        <dt>
            Accessory Description
        </dt>

        <dd>
            @Model.Accessories_Description
        </dd>

        <dt>
            Accessory Price
        </dt>

        <dd>
            @Model.Accessories_Price
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
