@model IEnumerable<Quest4pet.Models.tblAdmin>

@*@{
    ViewBag.Title = "Index";
}*@

<h2>Index</h2>

<p>
    <a class="btn btn-info fa fa-pencil" href="@Url.Action("Create","tbladmins")" > Create New</a>

    @*@Html.ActionLink("Create New", "Create")*@
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Admin_Name)*@
            Admin Name
        </th>
        <th>
            Admin Password
        </th>
        <th>
            Admin Email
        </th>
        <th>
            Admin Contact
        </th>
        <th></th>
    </tr>

@foreach (var admin in Model) {
    <tr>
        <td>
            @*@Html.DisplayFor(modelItem => item.Admin_Name)*@
            @admin.Admin_Name
        </td>
        <td>
            @admin.Admin_Password
        </td>
        <td>
            @admin.Admin_Email
        </td>
        <td>
            @admin.Admin_Contact
        </td>
        <td>
            <a href=@Url.Action("Edit","tblAdmins", new { id=admin.Admin_ID }) class="fa fa-edit btn btn-warning"> </a>
            <a href=@Url.Action("Details", new { id=admin.Admin_ID }) class="fa fa-book btn btn-primary"> </a>
            <a href=@Url.Action("Delete", new { id=admin.Admin_ID }) class="fa fa-trash-o btn btn-danger"> </a>

            @*@Html.ActionLink("Edit", "Edit", new { id=item.Admin_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Admin_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Admin_ID })*@
        </td>
    </tr>
}

</table>
